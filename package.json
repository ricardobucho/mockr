{
  "name": "mockr",
  "private": "true",
  "scripts": {
    "build:css": "sass ./app/assets/stylesheets/application.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules --quiet-deps --silence-deprecation=import",
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
    "watch:css": "yarn build:css --watch",
    "watch": "yarn build --watch"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.12",
    "@popperjs/core": "^2.11.8",
    "@rails/ujs": "^7.1.3",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "codemirror": "^5.65.16",
    "esbuild": "^0.25.0",
    "prismjs": "^1.29.0",
    "sass": "^1.83.1",
    "sortablejs": "^1.15.2"
  }
}
